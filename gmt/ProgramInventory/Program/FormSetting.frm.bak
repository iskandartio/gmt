Attribute VB_Name = "FormSetting"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Dim x As New XArrayDB
Dim y As New XArrayDB
Dim col1 As TrueOleDBGrid70.Columns
Dim coly As TrueOleDBGrid70.Columns
Dim iNoAccount As Integer
Dim iKetAcc As Integer
Dim iSaldoAwal As Integer
Dim iUpdated As Integer
Dim icNoAccount As Integer
Dim LNoAccount As Boolean

Private Sub fDelete_Click()
    b = MsgBox("Yakin mau dihapus?", vbYesNo)
    If b = vbNo Then Exit Sub
    a = "delete from t_LaporanSetting where NoAccount='" & esc(col1(icNoAccount).Value) & "'"
    If ExecMe(a) = 0 Then
        MsgBox "GAGAL"
        Exit Sub
    End If
    DoQuery
End Sub

Private Sub Form_Load()
    iNoAccount = 0
    iKetAcc = 1
    iSaldoAwal = 2
    iUpdated = 3
    icNoAccount = 4
    Set TDBGrid1.Array = x
    Set TDBDropDown1.Array = y
    Set col1 = TDBGrid1.Columns
    Set coly = TDBDropDown1.Columns
    TDBGrid1.AllowAddNew = True
    TDBGridSetLock TDBGrid1, iKetAcc, True
    TDBGridSetVisible TDBGrid1, iUpdated & "@" & icNoAccount
    col1(iNoAccount).DropDown = TDBDropDown1
    col1(iNoAccount).AutoDropDown = True
    col1(iSaldoAwal).Tag = "Decimal"
    TDBGridLoad TDBGrid1
    TDBGrid1.FetchRowStyle = True
    DoQuery
End Sub

Private Sub Form_Resize()
On Error Resume Next
    TDBGrid1.Width = ScaleWidth - TDBGrid1.Left - 100
    TDBGrid1.Height = ScaleHeight - TDBGrid1.Top - 100
End Sub

Private Sub fUpdate_Click()
On Error GoTo err
    BeginTransaction
    TDBGrid1.Update
    For i = 0 To x.UpperBound(1)
        If x(i, iUpdated) = 1 Then
            If x(i, icNoAccount) = "" Then
                a = "insert into t_LaporanSetting(NoAccount, KetAcc, n0, Tipe) values('" & _
                    esc(x(i, iNoAccount)) & _
                    "','" & esc(x(i, iKetAcc)) & _
                    "'," & cNum(x(i, iSaldoAwal)) & _
                    "," & cNum(IIf(Options(0).Value, 0, 1)) & ")"
                If ExecMe(a) = 0 Then GoTo err
            Else
                a = "update t_LaporanSetting set " & _
                    "NoAccount='" & esc(x(i, iNoAccount)) & _
                    "', KetAcc='" & esc(x(i, iKetAcc)) & _
                    "', n0=" & cNum(x(i, iSaldoAwal)) & _
                    ", Tipe=" & cNum(IIf(Options(0).Value, 0, 1)) & " where NoAccount='" & x(i, icNoAccount) & "'"
                If ExecMe(a) = 0 Then GoTo err
            End If
        End If
    Next
    CommitTransaction
    MsgBox "SUKSES"
    DoQuery
    Exit Sub
err:
    RollBackTransaction
    MsgBox "GAGAL"
End Sub

Private Sub Options_Click(Index As Integer)
    DoQuery
End Sub

Private Sub DoQuery()
    TDBGridClear TDBGrid1
    a = "select m_ChartAccount.NoAccount, Deskripsi, n0, 0, t_LaporanSetting.NoAccount from t_LaporanSetting left join m_ChartAccount on t_LaporanSetting.NoAccount=m_ChartAccount.NoAccount where Tipe=" & cNum(IIf(Options(0).Value, 0, 1)) & " order by Level1, Level2, Level3, Level4, Level5"
    query a
    If RS.RecordCount > 0 Then x.LoadRows RS.GetRows
    TDBGrid1.Rebind
    TDBGridSetVisible TDBGrid1, iSaldoAwal, Index = 0
End Sub
Private Sub TDBDropDown1_DropDownClose()
On Error Resume Next
    col1(iNoAccount).Value = coly("No Account").Value
    col1(iKetAcc).Value = coly("Keterangan").Value
End Sub

Private Sub TDBDropDown1_DropDownOpen()
    If Not LNoAccount Then
        a = "select NoAccount, Deskripsi from m_ChartAccount order by Level1, Level2, Level3, Level4, Level5"
        query a
        y.ReDim 0, 0, 0, coly.Count - 1
        y.DeleteRows 0
        If RS.RecordCount > 0 Then y.LoadRows RS.GetRows
        TDBDropDown1.Rebind
        LNoAccount = True
    End If
End Sub

Private Sub TDBDropDown1_Paint()
On Error Resume Next
    TDBGrid1.SelLength = Len(col1(TDBGrid1.Col).Text)
End Sub

Private Sub TDBGrid1_AfterColEdit(ByVal ColIndex As Integer)
On Error Resume Next
    col1(iUpdated).Value = 1
End Sub

Private Sub TDBGrid1_FetchRowStyle(ByVal Split As Integer, Bookmark As Variant, ByVal RowStyle As TrueOleDBGrid70.StyleDisp)
On Error Resume Next
    If x(Bookmark, iUpdated) = 1 Then RowStyle.BackColor = vbYellow
End Sub
