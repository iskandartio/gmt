Attribute VB_Name = "FormMasterKeterangan"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Dim col1 As TrueOleDBGrid70.Columns
Dim x As New XArrayDB
Dim iUpdated As Integer
Dim icId As Integer
Dim iId As Integer
Dim iKet As Integer

Private Sub fDelete_Click()
    If col1(icId).Value = "" Then Exit Sub
    a = "delete from m_Keterangan where IdKet=" & col1(icId).Value
    ExecMe a
    DoQuery
End Sub


Private Sub fKet_KeyPress(KeyAscii As Integer)
    col1(iUpdated).Value = 1
End Sub

Private Sub fKet_Validate(Cancel As Boolean)
    col1(iKet).Value = fKet
End Sub

Private Sub Form_Activate()
On Error Resume Next
    If Not cekValid("MASUK", Tag) Then Unload Me
End Sub

Private Sub DoQuery()
    a = "select 0, IdDet, IdKet, Keterangan from m_Keterangan order by IdKet"
    query a
    If RS.RecordCount > 0 Then x.LoadRows RS.GetRows
    TDBGrid1.Rebind
End Sub

Private Sub Form_Load()
    iUpdated = 0
    icId = 1
    iId = 2
    iKet = 3
    Caption = Caption & "---" & pTipe
    Set col1 = TDBGrid1.Columns
    Set TDBGrid1.Array = x
    TDBGrid1.AllowAddNew = True
    TDBGridClear TDBGrid1
    DoQuery
    TDBGridSetVisible TDBGrid1, iUpdated & "@" & icId
    TDBGrid1.FetchRowStyle = True
End Sub

Private Sub Form_Resize()
On Error Resume Next
    TDBGrid1.Height = ScaleHeight - TDBGrid1.Top - 100
    fKet.Width = ScaleWidth - fKet.Left - 100
    fKet.Height = TDBGrid1.Height
End Sub

Private Sub fUpdate_Click()
On Error GoTo err
    BeginTransaction
    TDBGrid1.Update
    For i = 0 To x.UpperBound(1)
        If x(i, iUpdated) = 1 Then
            If x(i, icId) <> "" Then
                a = "update m_Keterangan set IdKet=" & x(i, iId) & _
                    ", Keterangan='" & esc(x(i, iKet)) & _
                    "' where IdDet=" & x(i, icId)
                ExecMe a
            Else
                a = "insert into m_Keterangan(IdKet, Keterangan) values(" & _
                    x(i, iId) & _
                    ",'" & esc(x(i, iKet)) & "')"
                ExecMe a
            End If
        End If
    Next
    CommitTransaction
    MsgBox "SUKSES"
    DoEvents
    DoQuery
    Exit Sub
err:
    CommitTransaction
    MsgBox "GAGAL"
End Sub

Private Sub TDBGrid1_AfterColEdit(ByVal ColIndex As Integer)
On Error Resume Next
    col1(iUpdated).Value = 1
End Sub

Private Sub TDBGrid1_FetchRowStyle(ByVal Split As Integer, Bookmark As Variant, ByVal RowStyle As TrueOleDBGrid70.StyleDisp)
    If x(Bookmark, iUpdated) = 1 Then
        RowStyle.BackColor = vbYellow
    End If
End Sub

Private Sub TDBGrid1_RowColChange(LastRow As Variant, ByVal LastCol As Integer)
    fKet = col1(iKet).Value
End Sub
