Attribute VB_Name = "FormDetailKG"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Dim x As New XArrayDB
Public r_Kgs As String
Public r_box As Long
Public r_kg As Double
Public m_jumlahBox  As Long

Public Sub LoadMe(ByVal tBox As Integer, ByVal tKgs As String)
    Dim d1() As String
    Dim i As Long
    d1 = Split(tKgs, "_")
    x.Clear
    If tBox <= 10 Then
        x.ReDim 0, 0, 0, tBox - 1
    
    Else
        x.ReDim 0, tBox \ 10, 0, 9
    End If
    m_jumlahBox = tBox
    For i = 0 To TDBGrid1.Columns.count - 1
        TDBGrid1.Columns(i).Caption = i + 1
        TDBGrid1.Columns(i).Width = 600
        TDBGrid1.Columns(i).Tag = "Decimal"
        
        TDBGrid1.Splits(0).Columns(i).FetchStyle = dbgFetchCellStyleColumn
    
    Next
    
    i = 0
    j = 0
    For k = 0 To UBound(d1)
       x(i, j) = d1(k)
       j = j + 1
       If j = 10 Then
            i = i + 1
            j = 0
        End If
    Next
    
    
    TDBGrid1.Array = x
    TDBGrid1.Rebind
    Me.Show vbModal
End Sub

Private Sub TDBGrid1_FetchCellStyle(ByVal Condition As Integer, ByVal Split As Integer, Bookmark As Variant, ByVal Col As Integer, ByVal CellStyle As TrueOleDBGrid80.StyleDisp)
    If Bookmark * 10 + Col + 1 > m_jumlahBox Then
        CellStyle.BackColor = 0
        CellStyle.Locked = True
    End If
    
End Sub

Private Sub Form_Resize()
    TDBGrid1.Height = ScaleHeight - TDBGrid1.Top - 100
    
End Sub

Private Sub Form_Unload(Cancel As Integer)
On Error GoTo err
    TDBGrid1.Update
    r_Kgs = ""
    r_box = 0
    r_kg = 0
    For i = 0 To x.UpperBound(1)
        For i2 = 0 To x.UpperBound(2)
            If x(i, i2) <> "" Then
                r_Kgs = r_Kgs & "_" & x(i, i2)
                r_box = r_box + 1
                r_kg = r_kg + x(i, i2)
            End If
            
        Next
        
    Next
    r_Kgs = Mid(r_Kgs, 2)
    Exit Sub
err:
Cancel = True
End Sub

Private Sub TDBGrid1_KeyDown(KeyCode As Integer, Shift As Integer)
    If KeyCode = 27 Then
        Unload Me
    End If
    If KeyCode = 13 Then
        If TDBGrid1.Col = 9 Then
            If TDBGrid1.Row <= x.UpperBound(1) Then
                TDBGrid1.Row = TDBGrid1.Row + 1
                TDBGrid1.Col = 0
                KeyCode = 0
            End If
        End If

    End If
    
End Sub

Private Sub TDBGrid1_KeyPress(KeyAscii As Integer)
    TDBGridKeyPress TDBGrid1, KeyAscii
End Sub

